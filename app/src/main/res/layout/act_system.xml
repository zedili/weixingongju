<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_height="match_parent">

    <com.longrenzhu.common.widget.common.ItemClickWidget
        android:id="@+id/vWidgetLl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:zt_title="流量设置"
        app:zt_notNull="false"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_marginStart="@dimen/dp_10"
        android:layout_marginEnd="@dimen/dp_10"
        app:zt_layoutId="@layout/widget_item_click"/>

    <com.longrenzhu.common.widget.common.ItemClickWidget
        android:id="@+id/vWidgetWifi"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:zt_title="Wif设置"
        app:zt_notNull="false"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_marginStart="@dimen/dp_10"
        android:layout_marginEnd="@dimen/dp_10"
        app:zt_layoutId="@layout/widget_item_click"/>

    <com.longrenzhu.common.widget.common.ItemClickWidget
        android:id="@+id/vWidgetSystemSet"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:zt_title="系统设置"
        app:zt_notNull="false"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_marginStart="@dimen/dp_10"
        android:layout_marginEnd="@dimen/dp_10"
        app:zt_layoutId="@layout/widget_item_click"/>

    <com.longrenzhu.common.widget.common.ItemClickWidget
        android:id="@+id/vWidgetZm"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:zt_title="桌面设置"
        app:zt_notNull="false"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_marginStart="@dimen/dp_10"
        android:layout_marginEnd="@dimen/dp_10"
        app:zt_layoutId="@layout/widget_item_click"/>

</LinearLayout>