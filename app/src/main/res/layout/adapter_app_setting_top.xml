<?xml version="1.0" encoding="utf-8"?>
<com.longrenzhu.base.widget.shape.layout.ShapeLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:shape_solidColor="@color/cl_01C6AC"
    app:shape_radius="@dimen/dp_10"
    android:layout_height="wrap_content">


    <com.longrenzhu.base.widget.common.ratio.RatioLinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:zt_ratio="1"
        android:gravity="center">

        <TextView
            android:id="@+id/vTvName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/sp_16"
            android:textColor="@color/white"/>

    </com.longrenzhu.base.widget.common.ratio.RatioLinearLayout>

</com.longrenzhu.base.widget.shape.layout.ShapeLinearLayout>